# Complete the beautifulTriplets function below.
def beautifulTriplets(d, arr):
    arr.sort()
    count=0
    for i in range(len(arr)):
        if((arr[i]+d) in arr and (arr[i]+2*d) in arr):
            count+=1
    return count
